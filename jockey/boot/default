PROMPT 1
TIMEOUT 5
DEFAULT _DEFAULT
LABEL local
	KERNEL chain.c32
	APPEND hd0 0
LABEL firmware
	KERNEL memdisk
	APPEND initrd=freedos
LABEL memtest
	KERNEL memtest
LABEL detect
	KERNEL hdt.c32
	APPEND modules=hdt/modules.pcimap pciids=hdt/pci.ids memtest=memtest
LABEL centos6.3
	MENU LABEL centos6.3
	KERNEL centos6.3/vmlinuz
	APPEND initrd=centos6.3/initrd.img ksdevice=eth0 method=cdrom ks=http://KICKSTART
LABEL esxi
	MENU LABEL esxi
	KERNEL mboot.c32
	APPEND -c esxi/boot.cfg
